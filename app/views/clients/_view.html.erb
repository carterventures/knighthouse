<dl class="dl-horizontal">
	<dt>Id</dt>
	<dd><%= customer.id %> <small>(old: <%= customer.old_id %>)</small></dd>
	<dt>Name</dt>
	<dd><%= customer.first_name %> <%= customer.last_name %></dd>
	<dt>Phone #:</dt>
	<dd><%= customer.phone %></dd>
	<dt>Date of Birth</dt>
	<dd><%= customer.date_of_birth %></dd>
	<dt>Height</dt>
	<dd><%= customer.height %></dd>
	<dt>Weight</dt>
	<dd><%= customer.weight %></dd>
	<dt>Referrer Name</dt>
	<dd><%= customer.referrer_name %></dd>
</dl>